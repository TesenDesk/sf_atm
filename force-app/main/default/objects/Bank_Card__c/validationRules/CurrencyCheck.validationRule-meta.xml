<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CurrencyCheck</fullName>
    <active>true</active>
    <errorConditionFormula>Currency__c != &quot;RUB&quot; &amp;&amp; Currency__c != &quot;USD&quot; &amp;&amp; Currency__c != &quot;BYN&quot;</errorConditionFormula>
    <errorDisplayField>Currency__c</errorDisplayField>
    <errorMessage>This Currency is not supported</errorMessage>
</ValidationRule>
